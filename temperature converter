unit = input('temperature in Celcius of Farenheit? (C/F): ')

temp = float(input('Enter Temperature: '))

if unit == 'C':
    temp = round((9 * temp) / 5 + 32)
    print(f'The temperature is at {temp} degrees Fahrenheit')
elif unit == 'F':
    temp = round((temp-32)* 5/9)
    print(f'The temperature is at {temp} degrees Celcius')
else:
    print('invalid input, try again')
